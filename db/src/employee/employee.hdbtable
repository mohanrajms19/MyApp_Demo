COLUMN TABLE "employee" (
"id" BIGINT GENERATED BY DEFAULT AS IDENTITY,
"first_name" VARCHAR(100),
"gender" VARCHAR(100),
"marital_status" VARCHAR(100),
"dob" DATE,
"email" VARCHAR(100),
"mobile" VARCHAR(100),
"created_by" BIGINT ,
"created_on" DATETIME ,
"modified_by" BIGINT ,
"modified_on" DATETIME ,
"status" INTEGER  NOT NULL ,
PRIMARY KEY ("id")) UNLOAD PRIORITY 5 AUTO MERGE