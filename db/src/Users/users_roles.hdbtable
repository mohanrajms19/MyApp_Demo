COLUMN TABLE "users_roles" (
  "id" BIGINT GENERATED BY DEFAULT AS IDENTITY,
  "role_id"  BIGINT NOT NULL,
  "role_name" VARCHAR(200) NOT NULL,
  "created_by" BIGINT,
  "created_on" DATETIME,
  "modified_by" BIGINT,
  "modified_on" DATETIME,
  "status" INTEGER NOT NULL,
  PRIMARY KEY ("id")
) UNLOAD PRIORITY 2 AUTO MERGE